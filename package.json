{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "docker:up": "docker-compose up -d postgres",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d postgres",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:unit": "NODE_ENV=test jest tests/unit",
    "test:integration": "NODE_ENV=test jest tests/integration",
    "test:security": "NODE_ENV=test jest tests/security --maxWorkers=1",
    "test:performance": "NODE_ENV=test jest tests/performance --maxWorkers=1 --runInBand",
    "test:e2e": "NODE_ENV=test jest tests/e2e --maxWorkers=1",
    "test:openrouter": "NODE_ENV=test jest tests/integration/openrouter-api.test.js",
    "test:upload-security": "NODE_ENV=test jest tests/security/file-upload-security.test.js",
    "test:bulk-performance": "NODE_ENV=test jest tests/performance/bulk-upload-performance.test.js",
    "test:all": "NODE_ENV=test jest --coverage --maxWorkers=2",
    "test:ci": "NODE_ENV=test jest --ci --coverage --watchAll=false --maxWorkers=2",
    "test:quick": "NODE_ENV=test jest tests/unit tests/integration --coverage=false",
    "test:critical": "NODE_ENV=test jest tests/security tests/integration/openrouter-api.test.js --maxWorkers=1",
    "test:accuracy": "NODE_ENV=test jest --testNamePattern='accuracy|mapping'",
    "test:field-mapping": "NODE_ENV=test jest __tests__/unit/field-mapping-engine.test.ts",
    "test:field-extraction": "NODE_ENV=test jest __tests__/unit/field-extraction-service.test.ts",
    "test:enhanced-import": "NODE_ENV=test jest __tests__/unit/enhanced-import-service.test.ts",
    "test:workflow": "NODE_ENV=test jest __tests__/integration/complete-upload-workflow.test.ts",
    "test:error-recovery": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs",
    "test:error-recovery:watch": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --watch",
    "test:error-recovery:coverage": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --coverage",
    "test:error-recovery:unit": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --testPathPatterns=unit",
    "test:error-recovery:integration": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --testPathPatterns=integration",
    "test:error-recovery:performance": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --testPathPatterns=performance --maxWorkers=1",
    "test:error-recovery:api": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --testPathPatterns=api",
    "test:error-recovery:e2e": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --testPathPatterns=e2e --maxWorkers=1",
    "test:error-recovery:quick": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --testPathPatterns='integration.*error-recovery-integration'",
    "test:error-recovery:full": "NODE_ENV=test jest --config=jest.config.error-recovery.cjs --coverage --verbose"
  },
